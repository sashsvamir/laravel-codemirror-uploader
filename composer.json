{
    "name": "sashsvamir/laravel-codemirror-uploader",
    "description": "Laravel extension to turn your blade textarea component to Codemirror code editor with supporting uploading images",
    "keywords": ["laravel", "blade", "codemirror", "textarea", "wysiwyg", "upload", "images"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sashsvamir\\LaravelCodemirrorUploader\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Alexander Shchukin",
            "email": "1340068@gmail.com"
        }
    ],
    "require": {
        "sashsvamir/laravel-codemirror": "dev-main"
    },
    "extra": {
	    "laravel": {
	        "providers": [
	            "Sashsvamir\\LaravelCodemirrorUploader\\ServiceProvider"
	        ]
	    }
	}
}
